    "react-dom": "19.1.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.14",
    "@types/node": "^20.10.5",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "@types/sqlite3": "3.1.11",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "ansi-html-community": "0.0.8",
