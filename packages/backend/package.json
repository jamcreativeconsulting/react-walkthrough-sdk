    "events": "3.3.0",
    "html-entities": "2.6.0",
    "html-webpack-plugin": "5.6.3",
    "jest": "^29.7.0",
    "path-browserify": "1.0.1",
    "prettier": "^3.1.1",
    "stream-browserify": "3.0.0",
    "style-loader": "4.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "9.5.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.8.2",
    "util": "0.12.5",
    "webpack": "5.98.0",
    "webpack-cli": "5.1.4",
